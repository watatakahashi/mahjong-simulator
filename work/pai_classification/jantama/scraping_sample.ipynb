   "source": [
    "from requests_html import HTMLSession\n",
    "\n",
    "url = \"https://tenhou.net/2/?q=2335m4568p1258s6z6p\"\n",
    "\n",
    "# セッション開始\n",
    "session = HTMLSession()\n",
    "r = session.get(url)\n",
    "r.html.render()\n",
    "\n",
    "textarea_elem = r.html.find(\"textarea\", first=True)\n",
    "print(textarea_elem.html)   "
   ]
